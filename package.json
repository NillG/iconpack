{
  "name": "activecleaners-icon-pack",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "tsup src/icons --format esm,cjs --dts --out-dir dist",
    "build:icons": "rimraf src/icons && npx @svgr/cli --icon --typescript --out-dir src/icons ./icons && node generate-icon-index.js && tsup src/icons --format esm,cjs --dts --out-dir dist"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "rimraf": "^5.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.8.3"
  }
}